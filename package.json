{
  "name": "hello-world",
  "description": "An Ionic project",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "^3.5.1",
    "@capacitor/core": "3.5.1",
    "@capacitor/ios": "^3.5.1",
    "@ionic/react": "^5.9.4",
    "@ionic/react-router": "^5.9.4",
    "cordova-plugin-ionic": "5.5.2",
    "cordova-plugin-whitelist": "^1.3.5",
    "firebase": "^9.8.3",
    "ionicons": "^6.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.3",
    "styled-components": "^5.3.5",
    "typescript": "4.7.3"
  },
  "devDependencies": {
    "@capacitor/cli": "3.5.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.5.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.21",
    "@types/react": "^17.0.46",
    "@types/react-dom": "^17.0.17",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^7.32.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^5.0.6",
    "lint-staged": "^11.2.6",
    "prettier": "2.7.1",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:capacitor": "react-scripts build && npx cap copy && npx cap sync",
    "build:storybook": "build-storybook -s public",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.mjs,.jsx,.ts,.tsx src --color",
    "lint:fix": "eslint --ext .js,.mjs,.jsx,.ts,.tsx src --fix",
    "prettier:fix": "prettier --write ./src/**/*.*",
    "start": "react-scripts start",
    "start:android": "npx cap copy && npx cap open android",
    "start:electron": "npx cap copy && npx cap open @capacitor-community/electron",
    "start:ios": "npx cap copy && npx cap open ios",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006 -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/alt-text": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "react/jsx-no-target-blank": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
