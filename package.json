{
  "name": "hello-world",
  "description": "An Ionic project",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@capacitor-community/electron": "^1.3.3",
    "@capacitor/android": "^2.4.6",
    "@capacitor/core": "2.4.6",
    "@capacitor/ios": "^2.4.6",
    "@ionic/react": "^5.5.3",
    "@ionic/react-router": "^5.5.3",
    "cordova-plugin-ionic": "5.4.7",
    "cordova-plugin-whitelist": "^1.3.4",
    "firebase": "^9.8.3",
    "ionicons": "^5.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.6",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-controls": "^6.1.16",
    "@storybook/addon-docs": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/node-logger": "^6.1.16",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.16",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "eslint": "^7.19.0",
    "husky": "^5.0.6",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "react-scripts": "4.0.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:capacitor": "react-scripts build && npx cap copy && npx cap sync",
    "build:storybook": "build-storybook -s public",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.mjs,.jsx,.ts,.tsx src --color",
    "lint:fix": "eslint --ext .js,.mjs,.jsx,.ts,.tsx src --fix",
    "prettier:fix": "prettier --write ./src/**/*.*",
    "start": "react-scripts start",
    "start:android": "npx cap copy && npx cap open android",
    "start:electron": "npx cap copy && npx cap open @capacitor-community/electron",
    "start:ios": "npx cap copy && npx cap open ios",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006 -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/alt-text": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "react/jsx-no-target-blank": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
